html(lang='en')
  head
    block title
      title Your Celebrity Match

    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')

    link(rel='shortcut icon', href='/images/favicon.ico', type='image/x-icon')
    link(rel='icon', href='/images/favicon.ico', type='image/x-icon')

    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css')
    link(rel='stylesheet', href='/css/base.css')
    block extra-header

  body
    #wrap
      block header
      block content
      block fork-github
        a#github(href='https://github.com/watson-developer-cloud/yourcelebritymatch')
          img(style='position: absolute; top: 0; right: 0; border: 0;z-index:16;', src='https://camo.githubusercontent.com/a6677b08c955af8400f44c6298f40e7d19cc5b2d/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f677261795f3664366436642e706e67', alt='Fork me on GitHub', data-canonical-src='https://s3.amazonaws.com/github/ribbons/forkme_right_gray_6d6d6d.png')

      block footer
        #footer
          .container
            p.text-center
              a(href='/') Your Celebrity Match
              |&nbsp;|&nbsp;
              a(href='https://www.ibm.com/privacy/us/en/') Privacy Policy

    div#fb-root
    include share-scripts.jade

    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js')
    block extra-scripts
